package OOPS;

public class ObjectAndClassclassfile {
	
	String name;
	int age;
	  int rollno;
	String gender;
	
	
	
	public void setdata(String name, int age, int rolln, String gender){
		
		
		this.name = name;
		this.age = age;
		this.rollno = rolln;
		this.gender = gender;
	}
	
	@Override
	public String toString() {
		return "Name: " + name + ", Age: " + age + ", ID: " + rollno + ", Gender: " + gender;
	}
	
	public void dispalydata()
	{
		System.out.println(name);
		System.out.println(age);
		System.out.println(rollno);
		System.out.println(gender);
		
	}
}
